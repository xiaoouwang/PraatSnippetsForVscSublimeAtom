{
  "name": "awesomepraatsnippets",
  "publisher": "XiaoouWANGShanghai",
  "icon": "images/praat.png",
  "displayName": "awesomePraatSnippets",
  "description": "Best on market Praat Snippets for vscode by XiaoouWANG(xiaoouwang.github.io)",
  "repository": "https://github.com/xiaoouwang/PraatSnippetsForVscSublimeAtom",
  "version": "0.5.0",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "praat",
        "aliases": [
          "Praat",
          "praat"
        ],
        "extensions": [
          ".praat"
        ],
        "grammars": [
          {
            "language": "praat",
            "scopeName": "source.praat",
            "path": "./syntaxes/Praat.tmLanguage"
          }
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "praat",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
